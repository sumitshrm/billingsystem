<jsp:root xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:fn="http://java.sun.com/jsp/jstl/functions" xmlns:spring="http://www.springframework.org/tags" xmlns:form="http://www.springframework.org/tags/form" xmlns:jsp="http://java.sun.com/JSP/Page" version="2.0">
  <jsp:output omit-xml-declaration="yes" />
  <jsp:directive.attribute name="id" type="java.lang.String" required="true" rtexprvalue="true" description="The identifier for this tag (do not change!)" />
  <jsp:directive.attribute name="url" type="java.lang.String" required="true" rtexprvalue="true" description="Save and delete file sharing url" />


	<div class="form-group row">
			<label class="col-sm-2">Shared with: </label>
			<div class="col-sm-10">
         <div style="margin-top: 5px;" id='${id}'>
         </div>
			</div>
		</div>
<script>
var sharedMap = new Object(); // or var map = {};
    //sharedMap[myKey1] = myObj1;

    function getSharedMap(k) {
        return sharedMap[k];
    }
    var source = {
    		datatype: "json",
    		datafields: [{
    		  name: 'username'
    		}, {
    		  name: 'fullName'
    		},{
    		  name: 'id'
    		}],
    		url: "/logusers/json",
    		};
 		var dataAdapter = new $.jqx.dataAdapter(source, {
 		formatData: function (data) {
 		  if ($("#${id}").jqxComboBox('searchString') != undefined) {
 		      data.query = $("#${id}").jqxComboBox('searchString');
 		      return data;
 		  }
 		}
 		});
    		    
 		    $("#${id}").jqxComboBox({
 		    	 width: '100%',
 		         multiSelect: true,
 		         source: dataAdapter,
 		         remoteAutoComplete: true,
 		         autoDropDownHeight: true,
 		         minLength: 10,
 		         selectedIndex: 0,
 		         showArrow: false,
 		         displayMember: "fullName",
 		         valueMember: "username",
 		         search: function (searchString) {
 		             dataAdapter.dataBind();
 		         }
 		            
 		    }); 
 		    
 		   <c:forEach items="${sharedwith}" var="msheetShared">
 		  	$("#${id}").jqxComboBox('addItem',{username: "${msheetShared.sharedWith.username}",fullName: "${msheetShared.sharedWith.fullName}",id: "${msheetShared.sharedWith.id}"});
		    $("#${id}").jqxComboBox('selectItem', "${msheetShared.sharedWith.username}" );
		    sharedMap[${msheetShared.sharedWith.username}] = ${msheetShared.id};
 		   </c:forEach>
 		    
 		    $('#${id}').on('select', function (event){
	    		var args = event.args;
	    		if (args) {
	    			console.log(args);
	    			$.ajax({
	    				 type: "GET",
	    				 url: "${url}&amp;userid="+args.item.originalItem.id,
	    				 dataType: 'text',
	    				 data: {},
	    				 error: function(xhr, error){
	    				        alert("Some error occured :" + error);
	    				        location.reload(); 
	    				 },
	    				 success: function(result){
	    					 sharedMap[args.item.value] = result;
	    				 }
	    				});
 		    	}
 		    }); 
 		    $('#${id}').on('unselect', function (event) 
 		    		{
 		    		    var args = event.args;
 		    		    if (args) {
 		    		    	//console.log("original",args.item.originalItem.id);
 		    		    	var sharedid = getSharedMap(args.item.value);
 		    		    	//console.log(sharedMap);
 		    		    	//console.log(args.item.value);
 		    		    	console.log(sharedid);
 		    		    	$.ajax({
 			    				 type: "DELETE",
 			    				 url: "${url}&amp;shareid="+sharedid,
 			    				 dataType: 'text',
 			    				 data: {},
 			    				 error: function(xhr, error){
 			    					alert("Some error occured :" + error);
 			    					//location.reload(); 
 			    				 },
 			    				 success: function(result){
 			    					 //alert("success");
 			    				 }
 			    				});
 		    		}
 		    		});
 		    $("#${id}").jqxComboBox({validateSelection: function(itemValue)
 		        {
 		           return true;
 		        }                             
 		     }); 
 		   </script>
</jsp:root>